{
  "spec_dir": "dist",
  "spec_files": ["**/**/tests/*[sS]pec.?(m)js"],
  "helpers": ["tests/helpers/*.?(m)js"],
  "verboseDeprecations": true,
  "showColors": false,
  "reporters": [
    {
      "name": "jasmine-spec-reporter",
      "options": {
        "displayStacktrace": "all",
        "displaySuccessful": true,
        "verboseDeprecations": true
      }
    }
  ]
}
